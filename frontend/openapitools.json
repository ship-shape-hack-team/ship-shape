{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.2.0",
    "generators": {
      "typescript-axios": {
        "inputSpec": "../specs/003-quality-profiling-ui/contracts/openapi.yaml",
        "output": "./src/generated/api",
        "generatorName": "typescript-axios",
        "additionalProperties": {
          "supportsES6": true,
          "npmName": "@ship-shape/api-client",
          "npmVersion": "1.0.0",
          "withSeparateModelsAndApi": true,
          "apiPackage": "api",
          "modelPackage": "models"
        }
      }
    }
  }
}
